<?xml version="1.0" encoding="utf-8"?><!--  <include layout="@layout/scanner_toolbar"/>-->
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.dawn.baselib.view.zxing.activity.ZoomSurfaceView
        android:id="@+id/scanner_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center" />

    <com.dawn.baselib.view.zxing.view.ViewfinderView
        android:id="@+id/viewfinder_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:corner_color="@color/corner_color"
        app:frame_color="@color/viewfinder_frame"
        app:label_text="将二维码/条形码放入取景框内\n即可自动扫描"
        app:label_text_color="#ffffff"
        app:label_text_size="48"
        app:laser_color="@color/laser_color"
        app:mask_color="@color/viewfinder_mask"
        app:result_color="@color/result_view"
        app:result_point_color="@color/result_point_color" />

</FrameLayout>
